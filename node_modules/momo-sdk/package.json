{
  "name": "momo-sdk",
  "version": "0.1.0",
  "description": "momodani sdk",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "ava": "LOG_LEVEL=error NODE_ENV=test node --harmony ./node_modules/.bin/ava --verbose --serial",
    "test": "./node_modules/.bin/nyc -a --reporter=text --reporter=lcov --reporter=html npm run ava",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "build": "rm -rf ./lib && babel -d lib/ src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmqb/momodani-sdk.git"
  },
  "keywords": [
    "momodani",
    "momodani-sdk",
    "ES2015",
    "ES7"
  ],
  "author": "BMQB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmqb/momodani-sdk/issues"
  },
  "homepage": "https://github.com/bmqb/momodani-sdk#readme",
  "dependencies": {
    "request": "^2.75.0",
    "request-debug": "^0.2.0",
    "request-promise": "^4.1.1",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "isparta": "^4.0.0",
    "nyc": "^8.3.0",
    "sinon": "^1.17.6"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": false
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 4,
    "failFast": false,
    "tap": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text-lcov",
      "html"
    ],
    "babel": "inherit"
  }
}
